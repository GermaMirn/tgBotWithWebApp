{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["vite/client"],
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "target": "ES5",
    "lib": ["DOM", "ES5", "ES2017"],
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/types/**/*.d.ts"
  ]
}
