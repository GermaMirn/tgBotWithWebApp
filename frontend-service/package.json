{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix --color"
  },
  "dependencies": {
    "@twa-dev/sdk": "^8.0.2",
    "axios": "^1.11.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "pinia": "^2.3.1",
    "prettier": "^3.6.2",
    "primeicons": "^7.0.0",
    "primevue": "^3.53.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vue-tsc": "^2.2.12"
  },
	"husky":{
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged":{
		"*.ts":[
			"npm run lint:fix",
			"git add"
		],
		"*.vue":[
			"npm run lint:fix",
			"git add"
		]
	}
}
